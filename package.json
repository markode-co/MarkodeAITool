{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "cd client && pnpm exec vite",
    "build": "pnpm run build:client && pnpm run build:server && pnpm run build:copy",
    "build:server": "pnpm exec tsc -p server/tsconfig.json",
    "build:shared": "pnpm exec tsc -p shared/tsconfig.json",
    "build:client": "cd client && pnpm install --no-frozen-lockfile && pnpm exec vite build",
    "build:copy": "node build-copy.js",
    "start": "cross-env NODE_ENV=production PORT=$PORT tsx server/index.ts",
    "check": "pnpm exec tsc -p server/tsconfig.json --noEmit",
    "clean": "pnpm exec rimraf dist && pnpm exec rimraf client/dist",
    "preview": "cd client && pnpm run preview"
  },
